@using RoleProject.Models
@model  RoleProject.Models.Car
@{
    ViewBag.Title = "CancelRecive";
    ReciveDate reciveDate = ViewBag.ReciveDate;
    TempData["ReciveDate"] = reciveDate;
}

<h2>Cancel Recive</h2>
<h3>Are you sure you want to Cancel Recive?</h3>
<div>
    <h4>Car</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Type_Of_Car)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Type_Of_Car)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Car_Brand)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Car_Brand)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Car_Model)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Car_Model)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Chassis_No)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Chassis_No)
        </dd>



        <dt>
            @Html.DisplayNameFor(model => model.Agince_Of_Car.name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Agince_Of_Car.name)
        </dd>

        <dt>
            Client Name
        </dt>

        <dd>
            @reciveDate.client.Name
        </dd>
        <dt>
            From
        </dt>

        <dd>
            @reciveDate.Start_Recive_Date
        </dd>
        <dt>
            TO
        </dt>

        <dd>
            @reciveDate.End_Recive_Date
        </dd>




    </dl>

    @using (Html.BeginForm("CancelRecive", "Cars", FormMethod.Post, new { enctype = "multipart/form-data" } ))
    {
        @Html.AntiForgeryToken()

         
        <div class="form-actions no-color">
            <input type="submit" value="Cancel" class="btn btn-default" /> |
            @Html.ActionLink("Cancel", "CancelRecive","Cars")
            @Html.ActionLink("Back to List", "List_Of_All")
        </div>
    }
</div>


