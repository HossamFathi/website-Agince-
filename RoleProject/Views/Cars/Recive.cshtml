@using Microsoft.AspNet.Identity;
@model RoleProject.Models.Car
@{
    ViewBag.Title = "Recive";
}

<h2>Recive</h2>
@using (Html.BeginForm("Recive", "Cars", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
<div class="form-group">
    <label class="control-label col-md-2">Start Recive Date</label>
    <div class="col-md-10">
        <input type="datetime" name="Start_Recive_Date" id="Start_Recive_Date" class="form-control" placeholder="Start_Recive_Date"/>
       
    </div>
</div>

    <div class="form-group">
       
        <label class="control-label col-md-2">End Recive Date</label>
        <div class="col-md-10">
            <input type="datetime" name="End_Recive_Date" id="End_Recive_Date" class="form-control" placeholder="End_Recive_Date"/>
           
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.HiddenFor(model => model.Car_Id)

        </div>
    </div>


    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Recive" class="btn btn-default" />
        </div>
    </div>

}
<div>
    @Html.ActionLink("Back to List", "List_Of_all")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}